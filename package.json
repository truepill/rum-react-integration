{
  "name": "@truepill/rum-react-integration",
  "version": "1.1.0",
  "license": "Apache-2.0",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "cjs/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "node": "^22.11.0",
    "pnpm": "^9.14.2"
  },
  "scripts": {
    "build": "run-p build:cjs build:esm",
    "build:cjs": "rm -rf cjs && tsc -p tsconfig.cjs.json",
    "build:esm": "rm -rf esm && tsc -p tsconfig.esm.json",
    "format": "prettier --ignore-unknown --write src",
    "lint": "eslint --fix src",
    "typecheck": "scripts/cli typecheck . && scripts/cli typecheck developer-extension",
    "test": "vitest",
    "test:ci": "vitest -c vitest.config.ts"
  },
  "dependencies": {
    "tslib": "^1.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/truepill/rum-react-integration.git"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-router": "^7.1.1"
  },
  "devDependencies": {
    "@datadog/browser-rum-core": "^5.32.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@truepill/eslint-config-truepill": "^5.0.3",
    "@types/history": "^4",
    "@types/jest": "^26.0.24",
    "@types/node": "^22.10.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "7.32.0",
    "history": "^4",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-router": "^7.1.1",
    "unplugin-swc": "^1.4.4",
    "vite": "^5.4.11",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^1.6.0",
    "typescript": "^5.7.2"
  }
}
